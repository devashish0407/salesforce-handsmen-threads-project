<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Stock_Status__c</fullName>
    <description>This allows numerical comparisons like &gt; 10 in the formula.</description>
    <externalId>false</externalId>
    <formula>IF(Stock_Quantity__c &gt; 10, &quot;Available&quot;, &quot;Low Stock&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>This allows numerical comparisons like &gt; 10 in the formula.</inlineHelpText>
    <label>Stock Status</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
